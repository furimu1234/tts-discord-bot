{
	"compilerOptions": {
		"target": "es2020",
		"module": "esnext",
		"moduleResolution": "bundler",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"incremental": true,
		"tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"outDir": "dist"
	},
	"include": ["src/"]
}
