{
  "name": "example_build_mono",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "install": "pnpm -v",
    "build": "pnpm -F \"@example_build/*\" run build",
    "type-check": "pnpm -r --filter ./packages/* run type-check",
    "start": "pnpm -F \"@example_build/bot\" start",
    "dev": "pnpm -F \"@example_build/bot\" dev",
    "clean": "git clean -Xfd -e pnpm-lock.yaml",
    "generate": "pnpm --filter db run generate",
    "drizzle": "pnpm --filter db drizzle"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8"
  }
}
