{
  "name": "tts_discord_bot",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "install": "pnpm -v",
    "build": "pnpm -F \"@tts/*\" run build",
    "type-check": "pnpm -r --filter ./packages/* run type-check",
    "start": "pnpm -F \"@tts/bot\" start",
    "local": "pnpm build && pnpm start",
    "dev": "pnpm -F \"@tts/bot\" dev",
    "clean": "git clean -Xfd -e pnpm-lock.yaml",
    "generate": "pnpm --filter db run generate",
    "drizzle": "pnpm --filter db drizzle",
    "watch": "pnpm -r --filter \"@tts/db\" --filter \"@tts/lib\" run watch",
    "docker-clean": "docker compose down -v"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "dotenv": "^17.2.3",
    "drizzle-zod": "^0.8.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8"
  }
}
